n/*breakpoint*/\r\n$breakpoint: (\r\n    \"xs\": 0,\r\n    \"s\": 480px,\r\n    'md': 720px\r\n);\r\n\r\n/*mixins*/\r\n\r\n@mixin xs {\r\n    @media(min-width: map-get($breakpoint, 'xs')) {\r\n        @content;\r\n    }\r\n}\r\n@mixin xs {\r\n    @media(min-width: map-get($breakpoint, 's')) {\r\n        @content;\r\n    }\r\n}\r\n@mixin md {\r\n    @media(min-width: map-get($breakpoint, 'md')) {\r\n        @content;\r\n    }\r\n}\r\n\r\n","scss\\base.scss":"@use \"utilities\" as *;\r\n\r\n* {\r\n  padding: 0;\r\n  margin: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\nbody {\r\n  width: 100vw;\r\n  height: 100vh;\r\n  background-color: $Dark-Blue;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n@keyframes fade-in {\r\n  0% {\r\n    opacity: 0.1;\r\n  }\r\n  25% {\r\n    opacity: 0.3;\r\n  }\r\n  50% {\r\n    opacity: 0.5;\r\n  }\r\n  75% {\r\n    opacity: 0.8;\r\n  }\r\n  100% {\r\n    opacity: 1;\r\n  }\r\n}\r\n\r\n.container {\r\n  height: fit-content;\r\n  width: 90%;\r\n  background-color: $Dark-Grayish-Blue;\r\n  border-radius: $border-radius;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  position: relative;\r\n  animation: fade-in 0.6s ease-in forwards;\r\n  @include md {\r\n    width: 35%;\r\n  }\r\n}\r\n\r\nfooter {\r\n  position: fixed;\r\n  bottom: 10px;\r\n  height: 15px;\r\n  width: 100%;\r\n  color: $Light-Cyan;\r\n}\r\n","scss\\typography.scss":"\r\n@use 'utilities' as *;\r\n\r\nbody {\r\n    font-family: $font-family;\r\n}\r\n\r\nh1 {\r\n    color: $Neon-Green;\r\n    text-transform: uppercase;\r\n    font-size: $font-advice;\r\n    letter-spacing: 3.5px;\r\n}\r\n.quote {\r\n    color: $Light-Cyan;\r\n    font-size: $font-quote;\r\n}\r\n","scss\\components.scss":"@use 'utilities' as *;\r\n\r\nh1 {\r\n    margin: 3em auto 2em;\r\n}\r\n\r\n.divider {\r\n    background: url('../../images/pattern-divider-mobile.svg') no-repeat center center;\r\n    color: $Grayish-Blue;\r\n    width: 90%;\r\n    height: 15px;\r\n    margin: 2em auto 3em;\r\n    @include md {\r\n        background: url('../../images/pattern-divider-desktop.svg') no-repeat center center;\r\n        width: 80%;\r\n    }\r\n}\r\n\r\n.quote {\r\n    text-align: center;\r\n    margin: 0 1em;\r\n}\r\n\r\n.quote::before {\r\n    content: open-quote;\r\n}\r\n\r\n.quote::after {\r\n    content: close-quote;\r\n}\r\n\r\n.cube {\r\n    background-image: url('../../images/icon-dice.svg');\r\n    background-repeat: no-repeat;\r\n    background-position: center center;\r\n    background-size: 35%;\r\n    background-color: $Neon-Green;\r\n    border-radius: 50%;\r\n    border: none;\r\n    width: 50px;\r\n    height: 50px;\r\n    position: absolute;\r\n    top: 92%;\r\n}\r\n.cube:hover {\r\n    cursor: pointer;\r\n    box-shadow:  0 0 10px 10px $Neon-Green-Shadow;\r\n}\r\n\r\n.attribution { \r\n    font-size: 11px; \r\n    text-align: center; \r\n}\r\n\r\n.attribution a { \r\n    color: $Neon-Green;\r\n}"},"lineCount":null}},"error":null,"hash":"66063d1b8014ad4b01736e0ffa16afa9","cacheData":{"env":{}}}